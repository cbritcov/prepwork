html 															{ background: #ffffff; }
body 														{ font-family: Verdana, Helvetica, Sans-Serrif, Monospace; font-size: 12px; height: auto; width: 914px; margin: 0px auto; padding-left: 12px; }

a:link 														{ color: #001ebe; text-decoration: none; }
a:visited 													{ color: #001ebe; text-decoration: none; }
a:active 													{ color: #001ebe; text-decoration: none; }
a:hover 													{ color: #001ebe; text-decoration: none; }
a .navlink 													{ color: #001ebe; text-decoration: none; }

h1 															{  }
h2 															{ color: #333333; margin: 5px 3px 5px 3px; }
h3 															{ margin: 0px; padding: 0px; }
h4 															{  }
h5 															{  }
h6 															{  }

.pageTop 													{ background: url('../graphics/pageTop.png') no-repeat; width: 914px; height: 110px; padding-top: 12px; } 
.pageTop .header 										{ border-bottom: 1px solid #cccccc; /**background: url('../graphics/header.png') no-repeat;**/ width: 892px; height: 50px; margin-left: 12px; padding-top: 35px; } 
.pageTop .header .logo 								{ /**background: url('../graphics/logo.png') no-repeat; width: 255px; height: 54px; margin-left: 12px;**/ }
.pageTop .bar 											{ border-bottom: 1px solid #cccccc; /**background: url('../graphics/bar.png') no-repeat;**/ width: 892px; height: 22px; margin-left: 12px; }

.pageMiddle 												{ background: url('../graphics/pageMiddle.png') repeat-y; width: 914px; float: left; }
.pageMiddle .left 										{ float: left; width: 210px; height: auto; min-height: 400px; padding: 0px; }
.pageMiddle .left a 										{ margin-left: 12px; border: none; }
.pageMiddle .left a:hover 							{ border: none; }
.pageMiddle .button									{ margin-left: 12px; margin-bottom: 1px; width: 202px; height: 25px; padding-top: 7px; border: 1px solid #cccccc; text-align: center; }
.pageMiddle .button:hover							{ background: #ececec; }
.pageMiddle .button:active							{  }

.pageMiddle .left ul#menu, ul#menu ul 		{ list-style-type: none; margin: 0; padding: 0; width: 210px; } 
.pageMiddle .left ul#menu a 						{ display: block; text-decoration: none; } 
.pageMiddle .left ul#menu li 						{ margin-top: 1px; } 
.pageMiddle .left ul#menu li a 						{ background: #ffffff; color: #333333; border: 1px solid #cccccc; height: 25px; margin: 0px 0px 3px 12px; text-align: center; padding-top: 7px; width: 202px; } 
.pageMiddle .left ul#menu li a:hover 			{ background: #ececec; font-weight: bold; } 
.pageMiddle .left ul#menu li ul li a 				{ background: #ececec; color: #333333; padding-top: 7px; border: none; } 
.pageMiddle .left ul#menu li ul li a:hover 		{ background: #ffffff; border-left: 1px #cccccc solid; padding-top: 7px; } 
.pageMiddle .left 										{ float: left; width: 210px; height: auto; min-height: 200px; } 

.pageMiddle .right 										{ float: left; width: 680px; height: auto; min-height: 400px; margin-left: 10px; } 
.pageMiddle .right .contentHeader 				{ font-size: 24px; color: #001ebe; }

table th														{ width: 200px; text-align: left; }

.pageBottom 												{ background: url('../graphics/pageBottom.png') no-repeat; width: 914px; height: 100px; float: left; } 
.pageBottom .footer 									{ border-top: 1px solid #cccccc; /**background: url('../graphics/footer.png') no-repeat;**/ width: 892px; height: 42px; margin-left: 12px; margin-bottom: 100px; }

form 															{ font-size: 12px; }
form fieldset 												{ -webkit-border-radius: 8px; -moz-border-radius: 8px; }
form fieldset legend 									{ color: #000080; margin-left: 50px; }
form fieldset dl 											{ padding: 0px; margin: 0px; }
form fieldset dl dt 										{ width: 100px; float: left; margin: 0px; text-align: right; }
form fieldset dl dd 										{ width: 300px; float: left; margin-bottom: 5px; }
form fieldset dl dd input 								{ width: 300px; float: left; border: 1px solid #333333; } 
.submitButton 											{ width: 300px; border: 1px solid #333333; margin-left: 0px; }

.failure 														{ color: #a90000; } 
.success 													{ color: #74f019; } 

.error 														{ margin-top: 0px; padding-top: 0px; }

.breadCrumbs 											{ float: left; height: 20px; margin-top: 4px; margin-left: 210px; font-size: 12px; } 
.breadCrumbs .crumb 								{ color: #286be5; font-weight: bold; } 
.breadCrumbs .lastCrumb 							{ color: #001ebe; font-weight: bold; }

.wrapbox 													{ float: left; margin: 5px; }

.ui-accordion .ui-accordion-header 				{ cursor: pointer; position: relative; margin-top: 0px; margin-bottom: 3px; zoom: 1; background: #ffffff; border: 1px solid #cccccc; } 
.ui-accordion .ui-accordion-li-fix 					{ display: inline; } 
.ui-accordion .ui-accordion-header-active 	{ border-bottom: 0 !important; } 
.ui-accordion .ui-accordion-header a 			{ display: block; font-size: 12px; padding: 9px 5px 9px 5px; } 
.ui-accordion .ui-accordion-header .ui-icon 	{ position: absolute; left: .5em; top: 50%; margin-top: -8px; } 
.ui-accordion .ui-accordion-content 				{ padding: 1em 2.2em; border-top: 0; margin-top: -2px; position: relative; top: 1px; margin-bottom: 2px; overflow: auto; display: none; } 
.ui-accordion .ui-accordion-content-active 	{ display: block; }

#facebox .b 												{ background: url(b.png); }
#facebox .tl 												{ background: url(tl.png); }
#facebox .tr 												{ background: url(tr.png); }
#facebox .bl 												{ background: url(bl.png); }
#facebox .br 												{ background:url(br.png); }
#facebox 													{ position: absolute; top: 0; left: 0; z-index: 100; text-align: left; }
#facebox .popup 										{ position: relative; }
#facebox table 											{ border-collapse: collapse; }
#facebox td 												{ border-bottom: 0; padding: 0; }
#facebox .body 											{ border: 1px solid #ffffff; color: #ffffff; padding: 10px; background:url(b.png); width: 370px; }
#facebox .loading 										{ text-align: center; }
#facebox .image 										{ text-align: center; }
#facebox img 											{ border: 0; margin: 0; }
#facebox .footer 										{ border-top: 1px solid #DDDDDD; padding-top: 5px; margin-top: 10px; text-align: right; }
#facebox .tl, #facebox .tr, #facebox .bl, #facebox .br { height: 10px; width: 10px; overflow: hidden; padding: 0; }
#facebox_overlay 										{ position: fixed; top: 0px; left: 0px; height:100%; width:100%; }
.facebox_hide 											{ z-index:-100; }
.facebox_overlayBG 									{ background-color: #000; z-index: 99; }
* html #facebox_overlay 							{ /* ie6 hack */ position: absolute; height: expression(document.body.scrollHeight > document.body.offsetHeight ? document.body.scrollHeight : document.body.offsetHeight + 'px'); }